{
  "name": "fuzzaldrin-plus-fast",
  "version": "0.11.2",
  "description": "Fuzzaldrin plus - fast using native c bindings",
  "main": "fuzzaldrin.coffee",
  "scripts": {
    "prepare": "node-gyp-build",
    "prebuild": "prebuildify --napi",
    "test": "jasmine-node --coffee --captureExceptions spec",
    "benchmark": "coffee benchmark/benchmark.coffee",
    "benchmarklarge": "coffee benchmark/benchmark-large.coffee",
    "benchmarksmall": "node benchmark/benchmark-small.js",
    "bump": "ncu -u",
    "prepublishOnly": "npm run prebuild"
  },
  "gypfile": true,
  "dependencies": {
    "bindings": "~1.5.0",
    "node-addon-api": "~3.0.1",
    "node-gyp-build": "^4.2.3",
    "fuzzaldrin-plus": "^0.6.0"
  },
  "devDependencies": {
    "growl": ">=1.10.5",
    "jasmine-node": "^3.0.0",
    "prebuildify": "^4.0.0",
    "npm-check-updates": "8.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rajendrant/fuzzaldrin-plus-fast.git"
  },
  "author": "Rajendran T",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rajendrant/fuzzaldrin-plus-fast/issues"
  },
  "homepage": "https://github.com/rajendrant/fuzzaldrin-plus-fast"
}
